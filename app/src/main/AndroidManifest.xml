        android:theme="@style/AppTheme">
        <activity android:name=".Activity.Activity_loguin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.Activity_choosePlant" />
        <activity android:name=".Activity.Activity_inicio" />
        <activity android:name=".Activity.Activity_clima" />
        <activity android:name=".Activity.Activity_saludHuerto"></activity>
    </application>

</manifest>